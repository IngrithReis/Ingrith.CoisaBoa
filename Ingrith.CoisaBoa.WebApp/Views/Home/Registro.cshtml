@model UsuarioInputRegisterModel

@{
    ViewData["Title"] = "Cliente Novo? Bem-Vindo!";
}
<h1>@ViewData["Title"]</h1>
    <form asp-controller="Home" asp-action="Registro" method="post">
        
            <div class='row'>
                <div class='col-md-6'>
                    <div class="form-group">
                         <label asp-for="Nome"></label>
                         <input asp-for="Nome" class="form-control" />
                    </div>
                    <div class="form-group">
                         <label asp-for="Telefone"></label>
                         <input asp-for="Telefone" class="form-control" />
                    </div>
                </div>
              </div>
              <div class="form-group">
                         <label asp-for="Endereco"></label>
                         <input asp-for="Endereco" class="form-control" />
                    </div>
            <div class="form-group">
                         <label asp-for="Bairro"></label>
                         <input asp-for="Bairro" class="form-control" />
                    </div>
              <div class="form-group">
                         <label asp-for="Complemento"></label>
                         <input asp-for="Complemento" class="form-control" />
                    </div> 
            <button type="submit" class="btn btn-primary">Cadastrar</button>
        </form>






